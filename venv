pip install --upgrade stanze (to fix issue/warning during translation)

pip install shapely --upgrade (to fix issue ifL AttributeError: module 'shapely' has no attribute 'strtree')

pip install sphinx sphinx-copybutton myst-nb nbsphinx sphinx-rtd-theme
